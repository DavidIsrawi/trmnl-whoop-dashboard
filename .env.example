# Whoop API Credentials
WHOOP_CLIENT_ID=your_whoop_client_id
WHOOP_CLIENT_SECRET=your_whoop_client_secret

# The refresh token obtained during initial OAuth flow
# You'll need to update this if you persist it somewhere else, 
# but for a simple script, we'll need a way to store the latest refresh token.
WHOOP_REFRESH_TOKEN=your_initial_refresh_token

# TRMNL Configuration
TRMNL_WEBHOOK_URL=https://usetrmnl.com/api/custom_plugins/external_data/YOUR_ID
